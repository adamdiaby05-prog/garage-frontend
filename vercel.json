{
  "version": 2,
  "buildCommand": "bash vercel-build.sh",
  "outputDirectory": "build",
  "installCommand": "pnpm install --frozen-lockfile",
  "framework": "create-react-app",
  "routes": [
    { "handle": "filesystem" },
    { "src": "/.*", "dest": "/" }
  ],
  "env": {
    "NODE_ENV": "production",
    "REACT_APP_API_BASE_URL": "https://your-backend-url.vercel.app/api"
  }
}


